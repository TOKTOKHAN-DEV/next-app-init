{
  "name": "@toktokhan-dev/next-app-init",
  "version": "0.0.1",
  "description": "Simple project that shows the usage of Next.js with TypeScript",
  "author": "TOKTOKHAN.DEV<dev@toktokhan.dev>",
  "scripts": {
    "dev": "next dev",
    "temp-build": "next build",
    "type-check": "tsc --skipLibCheck --noEmit",
    "lint": "yarn type-check && next lint",
    "lint:staged": "lint-staged",
    "start": "yarn build && next start",
    "commit": "tokript commit -s ihih",
    "test": "jest --coverage --coverageReporters=text-summary",
    "test:watch": "jest --watch",
    "clean-up": "node src/scripts/remove-unnecessary.js",
    "tokript": "tokript2",
    "theme": "chakra-cli tokens src/configs/theme/index.ts",
    "theme:open": "code -r node_modules/@chakra-ui/styled-system/dist/theming.types.d.ts",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "semantic-release": "semantic-release",
    "checklist:progress": "node src/scripts/update-checklist-progress.js",
    "analyze": "ANALYZE=true next build",
    "changeset": "changeset"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.2.2",
    "@chakra-ui/color-mode": "^2.2.0",
    "@chakra-ui/layout": "^2.3.1",
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.5.1",
    "@chakra-ui/styled-system": "^2.9.1",
    "@chakra-ui/theme-tools": "^2.0.18",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^2.8.8",
    "@tanstack/react-query": "^5.27.3",
    "@tanstack/react-query-devtools": "^5.27.3",
    "@toktokhan-dev/react-universal": "^0.0.4",
    "@toktokhan-dev/react-web": "^0.0.11",
    "@toktokhan-dev/universal": "^0.0.4",
    "chakra-react-select": "^4.6.0",
    "dayjs": "^1.10.6",
    "framer-motion": "^10.5.0",
    "immer": "^10.0.1",
    "isomorphic-dompurify": "^2.12.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "next": "^14.2.0",
    "react": "^18.2.0",
    "react-device-detect": "^2.1.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-select": "^5.2.1",
    "tinycolor2": "^1.4.2",
    "use-context-selector": "^1.4.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.0",
    "@changesets/cli": "^2.27.5",
    "@next/bundle-analyzer": "^13.4.10",
    "@next/eslint-plugin-next": "^14.2.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@toktokhan-dev/cli": "^0.0.4",
    "@toktokhan-dev/cli-plugin-commit": "^0.0.3",
    "@toktokhan-dev/cli-plugin-gen-api-react-query": "^0.0.4",
    "@toktokhan-dev/cli-plugin-gen-img": "^0.0.4",
    "@toktokhan-dev/cli-plugin-gen-route-pages": "^0.0.4",
    "@toktokhan-dev/cli-plugin-gen-sitemap-next-page": "^0.0.4",
    "@toktokhan-dev/cli-plugin-gen-theme-chakra": "^0.0.4",
    "@toktokhan-dev/eslint-config": "^0.0.2",
    "@toktokhan-dev/prettier-config": "^0.0.1",
    "@toktokhan-dev/ts-config": "^0.0.1",
    "@types/facebook-pixel": "^0.0.30",
    "@types/gtag.js": "^0.0.12",
    "@types/jest": "^28.1.8",
    "@types/kakao-pixel": "^0.11.0",
    "@types/lodash": "^4.17.0",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "dotenv": "^16.0.0",
    "eslint": "^8.39.0",
    "husky": ">=7",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "engines": {
    "node": ">= 20.0.0",
    "npm": ">= 7.10.0",
    "yarn": ">= 1.22.17"
  },
  "os": [
    "darwin",
    "win64",
    "linux"
  ],
  "engineStrict": true
