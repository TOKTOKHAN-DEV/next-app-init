{
  "name": "@toktokhan-dev/next-app-init",
  "version": "0.0.8",
  "description": "Simple project that shows the usage of Next.js with TypeScript",
  "author": "TOKTOKHAN.DEV<dev@toktokhan.dev>",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "pnpm theme && next build",
    "analyze": "ANALYZE=true next build",
    "type-check": "tsc --skipLibCheck --noEmit",
    "lint": "pnpm type-check && npx eslint",
    "lint:staged": "lint-staged",
    "start": "pnpm build && next start",
    "test": "jest --coverage --coverageReporters=text-summary",
    "test:watch": "jest --watch",
    "clean-up": "node src/scripts/remove-unnecessary.js",
    "tokript": "tokript2",
    "commit": "tokript2 commit",
    "theme": "npx chakra typegen src/configs/theme/index.ts",
    "theme:open": "code -r node_modules/@chakra-ui/styled-system/dist/theming.types.d.ts",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "semantic-release": "semantic-release",
    "checklist:progress": "node src/scripts/update-checklist-progress.js",
    "changeset": "changeset",
    "prepare": "husky"
  },
  "dependencies": {
    "@chakra-ui/react": "3.28.1",
    "@emotion/react": "11.14.0",
    "@hookform/resolvers": "^3.0.0",
    "@next/third-parties": "15.5.6",
    "@tanstack/react-query": "^5.45.1",
    "@tanstack/react-query-devtools": "^5.45.1",
    "@toktokhan-dev/react-universal": "^0.0.12",
    "@toktokhan-dev/react-web": "^0.0.23",
    "@toktokhan-dev/universal": "^0.0.8",
    "@toktokhan-dev/zustand-react": "^0.0.3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "chakra-react-select": "6.0.0",
    "dayjs": "^1.11.11",
    "immer": "^10.1.1",
    "isomorphic-dompurify": "^2.12.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "next": "^15.5.6",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-cookie": "^7.1.4",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.52.0",
    "yup": "^1.4.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@chakra-ui/cli": "3.28.1",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.5",
    "@eslint/js": "^9.39.1",
    "@next/bundle-analyzer": "^15.5.6",
    "@next/eslint-plugin-next": "^15.5.6",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@toktokhan-dev/cli": "^0.0.11",
    "@toktokhan-dev/cli-plugin-commit": "0.0.10",
    "@toktokhan-dev/cli-plugin-gen-api-react-query": "0.1.3",
    "@toktokhan-dev/cli-plugin-gen-icon-chakra": "0.0.13",
    "@toktokhan-dev/cli-plugin-gen-img": "^0.0.11",
    "@toktokhan-dev/cli-plugin-gen-theme-chakra": "^0.0.17",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "20.14.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "globals": "^16.5.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.46.3"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "engines": {
    "node": ">= 20.0.0",
    "npm": ">= 7.10.0",
    "yarn": ">= 1.22.17",
    "pnpm": ">= 8.15.9"
  },
  "os": [
    "darwin",
    "win64",
    "linux"
  ],
  "engineStrict": true,
  "pnpm": {
    "patchedDependencies": {
      "lodash@4.17.21": "patches/lodash@4.17.21.patch"
    }
  }
}
